  "keywords": [
    "amqplibrary-ts",
    "amqp",
    "amqplib",
    "rabbitmq",
    "publish",
    "subscribe",
    "send",
    "listen",
    "request",
    "reply",
    "rpc"
  ],
  "author": "Gerald Campana",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/campanagerald/amqplibrary.git"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "del-cli": "^3.0.1",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/uuid": "^8.0.0",
    "amqplib": "^0.5.6",
    "uuid": "^8.1.0"
  }
