{
  "name": "amqplibrary-ts",
  "version": "1.0.4",
  "description": "Simple RabbitMQ library that built on top of amqplib written in Typescript",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "scripts": {
